version: "3"
services:
  postgresql:
    image: postgres:9
    container_name: postgresql
    volumes:
      - "<%= ENV["POSTGRES_DATA"] %>:/var/lib/postgresql/data"
    environment:
      - "POSTGRES_USER=<%= ENV["POSTGRES_USER"] %>"
      - "POSTGRES_PASSWORD=<%= ENV["POSTGRES_PASSWORD"] %>"
    ports:
      - "5432:5432"
